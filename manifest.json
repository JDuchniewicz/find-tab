{
    "description": "Navigate to the tab you need",
    "homepage_url": "https://www.github.com/JDuchniewicz/find-tab",
    "manifest_version": 2,
    "name": "Navigate to tab",
    "permissions": [
      "find",
      "tabs"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "icons": {
      "32": "search.svg"
    },
    "browser_action": {
      "browser_style": true,
      "default_title": "Navigate to tab",
      "default_icon": {
          "32": "search.svg"
      }
    },
    "version": "1.0",
    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "57.0a1"
      }
    },
    "commands": {
      "toggle-plugin": {
        "suggested_key": {
          "default": "Ctrl+Space"
        },
        "description": "Send a 'toggle-plugin' event to turn it on and off"
      }
    }
  }